<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">

    <service-credential service-credential-id="Shift4">
        <url>https://api.shift4.com</url>
    </service-credential>

    <service-profile service-profile-id="Shift4">
        <timeout-millis>30000</timeout-millis>
        <rate-limit-enabled>true</rate-limit-enabled>
        <rate-limit-calls>5</rate-limit-calls>
        <rate-limit-millis>100</rate-limit-millis>
        <cb-enabled>true</cb-enabled>
        <cb-calls>5</cb-calls>
        <cb-millis>500</cb-millis>
    </service-profile>

    <service service-id="Shift4">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>Shift4</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>Shift4</profile-id>
        <credential-id>Shift4</credential-id>
    </service>

</services>
